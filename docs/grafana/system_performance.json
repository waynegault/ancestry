{
  "__requires": [
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": "9.5.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": "9.5.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "2.41.0"
    }
  ],
  "title": "Ancestry - System Performance & Health",
  "uid": "ancestry-performance",
  "editable": true,
  "tags": [
    "ancestry",
    "performance",
    "system"
  ],
  "timezone": "browser",
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "title": "System Health Overview",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "collapsed": false
    },
    {
      "title": "Session Active Time",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "ancestry-prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1800,
                "color": "yellow"
              },
              {
                "value": 3000,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "targets": [
        {
          "expr": "ancestry_session_uptime_seconds",
          "legendFormat": "Uptime",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Total API Calls Made",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "ancestry-prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 1
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "targets": [
        {
          "expr": "sum(ancestry_api_requests_total)",
          "legendFormat": "Total Requests",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Active Worker Threads",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "ancestry-prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 1
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              },
              {
                "value": 10,
                "color": "yellow"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "targets": [
        {
          "expr": "ancestry_worker_thread_count",
          "legendFormat": "Workers",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Session Refresh Count",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "ancestry-prometheus"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 1
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 5,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "targets": [
        {
          "expr": "sum(ancestry_session_refresh_total)",
          "legendFormat": "Refreshes",
          "refId": "A"
        }
      ]
    },
    {
      "title": "API Performance",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "collapsed": false
    },
    {
      "title": "API Response Time by Endpoint (95th Percentile)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "ancestry-prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max"
          ]
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ancestry_api_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "API Request Rate (Requests/Second)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "ancestry-prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 6
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max"
          ]
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(ancestry_api_requests_total[5m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "API Success Rate vs Errors",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "ancestry-prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": {
              "mode": "normal"
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "success"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "error"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(ancestry_api_requests_total[5m])) by (result)",
          "legendFormat": "{{result}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Average Response Time by Endpoint",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "ancestry-prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean"
          ]
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(ancestry_api_latency_seconds_sum[5m])) by (endpoint) / sum(rate(ancestry_api_latency_seconds_count[5m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Rate Limiting & Efficiency",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 22
      },
      "collapsed": false
    },
    {
      "title": "Rate Limiter Wait Time (Percentiles)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "ancestry-prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 23
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(ancestry_rate_limiter_delay_seconds_bucket[5m])) by (le))",
          "legendFormat": "50th percentile (median)",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(ancestry_rate_limiter_delay_seconds_bucket[5m])) by (le))",
          "legendFormat": "95th percentile",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(ancestry_rate_limiter_delay_seconds_bucket[5m])) by (le))",
          "legendFormat": "99th percentile",
          "refId": "C"
        }
      ]
    },
    {
      "title": "Actions Processed Over Time",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "ancestry-prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 23
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "sum"
          ]
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(ancestry_action_processed_total[5m])) by (action) or on() vector(0)",
          "legendFormat": "Action {{action}}",
          "refId": "A"
        }
      ]
    }
  ]
}