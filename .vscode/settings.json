{
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/Scripts/python.exe",
    "python.terminal.activateEnvironment": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.venv/**": true,
        "**/__pycache__/**": true,
        "**/Cache/**": true,
        "**/Logs/**": true,
        "**/Data/**": true,
        "**/*.db": true,
        "**/*.ged": true,
        "**/linter_errors*.txt": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/.venv": true,
        "**/__pycache__": true,
        "**/Cache": true,
        "**/Logs": true,
        "**/Data": true,
        "**/*.db": true,
        "**/*.ged": true,
        "**/linter_errors*.txt": true
    }
}
